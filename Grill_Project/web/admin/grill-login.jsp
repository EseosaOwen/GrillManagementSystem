<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SIgnIn Zillow</title>
    <link rel="stylesheet" href="admin/zillow.css">
</head>
<body>
    <div class="body">
        <div class="brand-name">
            <div class="fa-solid fa-circle" id="circle-svg"></div>
            <!-- <i class="fa-solid fa-bolt-lightning"></i> -->
            <h2>Grill</h2>
        </div>
        <div class="whole-img">
            <div class="login" id="login">
                <div class="second-login-side">
                    <div class="login-head">
                        <h3 class="head-choice">Login</h3>
                    </div>
                    <div class="response"></div>
                    <div class="login-signup">
                        <form action="login-control" class="form-login">
                            <div class="login-body">
                                <input type="email" class="email-box" placeholder="Email" name="email" required>
                                <div class="pass-eye">
                                    <div class="passwordMainBox">
                                        <input type="password" class="password-box" placeholder="Password" name="password" required>
                                    </div>
                                </div>
                            </div>
                            <button class="login-btn" type="submit">Log in</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer class="footer">
        <div class="footer-section">
            <div class="top-footer">
                <div class="first-word">
                    <div class="first">
                        <h3>Real Estate</h3>
                        <i class="fa-solid fa-angle-down"></i>
                    </div>
                </div>
                <div class="second-word">
                    <div class="second">
                        <h3>Rentals</h3>
                        <i class="fa-solid fa-angle-down"></i>
                    </div>
                </div>
                <div class="third-word">
                    <div class="third">
                    <h3>Mortgage Rates</h3>
                    <i class="fa-solid fa-angle-down"></i>
                    </div>
                </div>
                <div class="fourth-word">
                    <div class="fourth">
                    <h3>Browse Homes </h3>
                    <i class="fa-solid fa-angle-down"></i>
                    </div>
                </div>
            </div>
            <div class="middle-footer">
                <small>About</small>
                <small>Zestimates</small>
                <small>Research</small>
                <small>Carrer</small>
                <small>Help</small>
                <small>Advertise</small>
                <small>Fair Housing Guide</small>
                <div>
                    <br><small><a href="#">Do Not Sell My Personal Information<i class="fa-solid fa-arrow-right-long"></i></a></small>
                </div>
            </div>
        </div>
    </footer>
    <script src="zillow_2.js"></script>
</body>
</html>